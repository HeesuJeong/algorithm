import java.util.Scanner;

public class 과목평가1 {

	static int T;
	static char[][] arr;
	static int N;
	static int M;
	static Scanner sc = new Scanner(System.in);
	static int[] dx = { -1, 1, 0, 0 };
	static int[] dy = { 0, 0, -1, 1 };

	static boolean inArr(int x, int y) {
		return x >= 0 && x < N && y >= 0 && y < M;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		T = sc.nextInt();
		for (int tc = 1; tc <= T; tc++) {
			N = sc.nextInt();
			M = sc.nextInt();
			arr = new char[N][M];
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < M; j++) {
					arr[i][j] = sc.next().charAt(0);
				}
			}
			int resul = 0;
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < M; j++) {
					if (arr[i][j] == 'G') {
						resul += shoot(i, j);
						System.out.println("resul "+ resul);
					}
				}
			}
			System.out.println("#"+tc+" "+resul);
		} // tc
	}

	static int shoot(int x, int y) {
		int sum = 0;
		for (int i = 0; i < 4; i++) {
			int dirX=dx[i];
			int dirY=dy[i];
			int nx = x;
			int ny = y;
			System.out.println(nx+" "+ny);
			while (inArr(nx, ny) && (arr[nx][ny] == '0'||arr[nx][ny] == 'T')) {
				
					if(arr[nx][ny] == 'T') {
						sum++;
						break;
					}
					nx+=dirX;
					ny+=dirY;
			}
		}
		return sum;
	}
}
