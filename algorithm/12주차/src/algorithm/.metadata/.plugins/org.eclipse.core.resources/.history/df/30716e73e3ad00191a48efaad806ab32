import java.util.Arrays;
import java.util.Scanner;

public class _5215 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		int N=0; int K=0;
		int[] arr=new int[21];
		int CNT=0;
		
		for (int TC = 1; TC <= T; TC++) {
			N=sc.nextInt();
			K=sc.nextInt();
			for (int i = 0; i < N; i++) {
				arr[i]=sc.nextInt();
			}
			
		System.out.println(Arrays.toString(arr)+" 길이 "+N);
		for(int i=0;i<(1<<N);i++) {
			int sum=0;
			for(int j=0;j<N;j++) {
				
				if(((1<<j)&i)==1) {
					System.out.println("sum은 "+sum);
					sum+=arr[i];
				}
			}
			
			if(sum==K) CNT++;
		}
		
		System.out.println("testing "+CNT);
		}//TC
	}//main

}
