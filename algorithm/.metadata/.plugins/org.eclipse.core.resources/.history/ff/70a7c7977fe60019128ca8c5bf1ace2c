package 금요일;

import java.util.Scanner;
public class 의석이의우뚝선산2 {

	static int[] arr;
	static int result = 0;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int TC = sc.nextInt();
		for (int tc = 1; tc <= TC; tc++) {
			result = 0;
			int N = sc.nextInt();
			arr = new int[N];
			for (int i = 0; i < N; i++) {
				arr[i] = sc.nextInt();
			}
			int start=0;
			int a=0;
			int middle=0;
			int b=0;
			boolean chk=false;
			int result=0;
			for (int i = start; i <N-1; i++) {
				if(chk==false&&arr[i]>arr[i+1]) {
					a++;
				}else if(arr[i]<arr[i+1]) {
					chk=true;
				}else if(chk==true&&arr[i]>arr[i+1]) {
					middle=i;
					b++;
				}else if(chk==true&&arr[i]<arr[i+1]) {
					start=i;
					result+=(a*b);
				}
				chk=false;
			}
			System.out.println("#" + tc + " " + result);
		} //
	}

}
