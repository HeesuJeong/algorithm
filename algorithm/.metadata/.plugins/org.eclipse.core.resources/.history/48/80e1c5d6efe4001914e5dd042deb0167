import java.util.Arrays;
import java.util.Scanner;

public class 입국심사 {

	static int N;
	static int M;
	static int[] arr;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);

		int TC = sc.nextInt();
		for (int tc = 1; tc <= TC; tc++) {
			N = sc.nextInt();
			M = sc.nextInt();
			arr = new int[N];
			for (int i = 0; i < N; i++) {
				arr[i] = sc.nextInt();
			}
			Arrays.sort(arr);
			int min = arr[0];
			int max = arr[N-1] * M;
			while (min <= max) {
				int middle = (min + max) / 2;
				long num = 0;
				for (int i = 0; i < arr.length; i++) {
					num += (long) middle / arr[i];
				}
				if (num < M) {
					min = middle+1;
				} else if (num >= M) {
					//크거나 같으면 앞을봐
					max = middle-1;
				}
			}
			System.out.println("#"+tc+" "+min);
		} // tc
	}

}
